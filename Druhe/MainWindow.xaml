<Window x:Class="Druhe.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Druhe"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
	<Grid>
		<ComboBox x:Name="CmBoxFuncNames" HorizontalAlignment="Left" Margin="30,30,0,0" VerticalAlignment="Top" Width="120" />
		<TextBox x:Name="TxtXvalue"
		         HorizontalAlignment="Left" Height="23" Margin="30,60,0,0" TextWrapping="Wrap" VerticalAlignment="Top"
		         Width="120" ToolTip="Zadejte hodnut X" />
		<TextBox x:Name="TxtYvalue"
		         HorizontalAlignment="Left" Height="23" Margin="30,90,0,0" TextWrapping="Wrap" VerticalAlignment="Top"
		         Width="120" IsReadOnly="True" />
		<Button HorizontalAlignment="Left" Margin="30,120,0,0" Content="Zjistit číslo" VerticalAlignment="Top" Click="GetVal_OnClick" />
	</Grid>
</Window>